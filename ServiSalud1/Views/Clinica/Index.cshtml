@model IEnumerable<ServiSalud1.Models.Clinica>
@{
    ViewData["Title"] = "Nuestras Clínicas";

}

<div class="text-center">
    <h1 class="display-4">Nuestras Clínicas</h1>
    <table>
        <tr>
            <th>
                Codigo
            </th>
            <th>
                Clínica
            </th>
            <th>
                Ubicacion
            </th>
        </tr>
        @foreach(var item in Model)
        {
            <tr>
                <td>
                    @item.Id_Clinica
                </td>
                <td>
                    @item.Nombre_clinica
                </td>
                <td>
                    @item.Ubicacion
                </td>
            </tr>
        }

    </table>
</div>


            

